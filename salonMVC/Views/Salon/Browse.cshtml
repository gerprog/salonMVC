@model salonMVC.Models.TreatmentType
@{
    ViewBag.TreatmentName = "Browse";
}

<h2>Browsing TreatmentType: @Model.Name</h2>
<ul>
    @foreach (var treatment in Model.Treatments)
    {
        <li>
            @Html.ActionLink(treatment.TreatmentName,
  "Details", new { id = treatment.TreatmentId })
        </li>
    }
</ul>
